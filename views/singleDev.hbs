<div class="developer">
    <div class="row">
        <div class="col s12 m7">
            <div class="card">
                <div class="card-image">
                    <img src="{{user.img}}" alt="{{user.firstName}}">
                    <span class="card-title">{{title}}</span>
                </div>
                <div class="card-content">
                    <p>{{user.email}}</p>
                </div>
                <div class="card-action">
                    <a href="#" class="price big">{{user.price}}</a>
                </div>
            </div>
        </div>
    </div>

</div>